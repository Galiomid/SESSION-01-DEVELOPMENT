pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                sh'''
            pwd
            nproc
            ls
                '''
            }
        }

    stage('Bonjour') {
            steps {
               sh'''
            mkdir jen
            touch jo
                '''
            }
        }

    stage('Hola') {
            steps {
                sh'''
            uname 
                '''
            }
        } 
    }
}
