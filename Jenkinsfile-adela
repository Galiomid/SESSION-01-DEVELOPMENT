pipeline {

    agent any
    properties([disableConcurrentBuilds(), buildDiscarder(logRotator(artifactDaysToKeepStr: '', artifactNumToKeepStr: '', daysToKeepStr: '3', numToKeepStr: '5'))])

    stages {
        stage('Hello') {
            steps {
                sh '''
                ls
                pwd
                '''
            }
        }

        stage('Hello1') {
            steps {
                sh '''
                uname -a
            
                '''
            }
        }




    }



}

