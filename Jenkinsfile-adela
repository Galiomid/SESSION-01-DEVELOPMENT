pipeline {

    agent any
    options {
    buildDiscarder(logRotator(numToKeepStr: '30', artifactNumToKeepStr: '30'))
  }

  }


    stages {
        stage('Hello') {
            steps {
                sh '''
                ls
                pwd
                '''
            }
        }

        stage('Hello1') {
            steps {
                sh '''
                uname -a
            
                '''
            }
        }




    }



}

