pipeline {
    agent any
properties([buildDiscarder(logRotator(artifactDaysToKeepStr: '', artifactNumToKeepStr: '', daysToKeepStr: '3', numToKeepStr: '5')), disableConcurrentBuilds()])

    stages {
        stage('Hello') {
            steps {
                sh '''
            ls
            pwd
            touch valdo
            mkdir Vald
                '''
            }
        }

        stage('Hello1') {
            steps {
                sh '''
                uname
                uptime
                uname -r
                '''
            }
        }

        stage('Hello2') {
            steps {
                sh '''
                date
            
                '''
                echo 'Hello World'
             }
                }





    }






}
